var threadedVar: Int = 0

fun add1000(): Int {
    var i : Int = 0
    while  i < 1000 {
        protect : {
            threadedVar = threadedVar + 1
        }
        i = i + 1
    }
    return 1
}

var threadedVarNoProtect : Int = 0
fun add1000NoProtect(): Int {
    var i : Int = 0
    while  i < 1000 {
        threadedVarNoProtect = threadedVarNoProtect + 1
        i = i + 1
    }
    return 1
}

