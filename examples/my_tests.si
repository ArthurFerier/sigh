var threadedVar: Int = 0

fun add1000(): Int {
        var i: Int = 0
        protect : {
            while i < 10000 {
                threadedVar = threadedVar+1
                i = i +1
            }
        }
        return 5
}


var returned : Int = launch add1000()

wait(returned)

print("bonjour")
print(""+returned)

