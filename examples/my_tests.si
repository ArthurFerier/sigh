var threadedVar: Int = 0

var i: Int = 0
fun add1000(): Int {
            while i < 10000 {
                protect : {
                    i = i + 1
                    threadedVar = threadedVar+1
                }
            }
        return 1
}




launch var returned : Int = add1000()
launch var returned2 : Int = add1000()

wait(returned)
wait(returned2)

print(""+returned)
print(""+returned2)
print(""+threadedVar)
