var threadedVar: Int = 0

fun add1000(): Int {
        var i: Int = 0
        protect : {
            while i < 10000 {
                threadedVar = threadedVar+1
                i = i +1
            }
        }
        return 5
}




launch var returned : Int = add1000()
launch var returned2 : Int = add1000()

wait(returned)
wait(returned2)

print(""+returned)
print(""+returned2)
print(""+threadedVar)